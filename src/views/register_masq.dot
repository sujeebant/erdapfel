<div class="register_masq_panel">
  {{? (!this.isRegistered)}}
    <h1 class="register_masq_panel__title">
      {{= _('Register with masq', 'masq') }}
    </h1>
    <p>
      {{= _('We use MASQ to keep your personal data safe', 'masq') }}
    </p>
    <br>
    <button class="register_masq_panel__action" {{= click(this.register, this) }}>
      {{= _('Register', 'masq') }}
    </button>
  {{?}}
  {{? this.isRegistered}}
    <p class="loggedin_masq_panel__title">
      {{= this.username }}
    </p>
    <button class="register_masq_panel__action" {{= click(this.unregister, this) }}>
      {{= _('Logout', 'masq') }}
    </button>
  {{?}}
</div>
